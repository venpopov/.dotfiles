{
    "restore_source_documents": false,
    "save_workspace": "never",
    "load_workspace": false,
    "initial_working_directory": "~",
    "restore_last_project": false,
    "posix_terminal_shell": "zsh",
    "editor_theme": "Monokai",
    "default_project_location": "~",
    "jobs_tab_visibility": "shown",
    "default_open_project_location": "~/Sandbox",
    "insert_native_pipe_operator": true,
    "highlight_selected_line": true,
    "relative_line_numbers": true,
    "margin_column": 120,
    "scroll_past_end_of_document": true,
    "highlight_r_function_calls": true,
    "code_formatter": "styler",
    "syntax_color_console": true,
    "help_font_size_points": 12,
    "font_size_points": 12,
    "visual_markdown_editing_is_default": true,
    "soft_wrap_r_files": true,
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "TabSet2",
            "Console"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": false,
        "additional_source_columns": 0
    },
    "rmd_viewer_type": "pane"
}